/*

    coroutines

*/



def subroutine() yield
{
    yield 3;
    yield 5;
    yield 9;
}


def coroutine( x ) yield
{
    x = yield( x * 2 );
    subroutine() yield;
    x = yield( x * 3 );
    x = yield( x * 4 );
}



var c = coroutine();
printf( "%g\n", c( 4 ) );
printf( "%g\n", c( 5 ) );
printf( "%g\n", c( 6 ) );





